#!/bin/bash
#SBATCH -A gpu
#SBATCH --gres=gpu:1
#SBATCH --cpus-per-task=5
#SBATCH --nodes=1
#SBATCH --job-name mnist_-1-0-0-300-mlp_0-0-sgd-0.0001-0.
#SBATCH --time=30:00
#SBATCH --output logs/mnist_-1-0-0-300-mlp_0-0-sgd-0.0001-0./output
#SBATCH --error logs/mnist_-1-0-0-300-mlp_0-0-sgd-0.0001-0./error
python main.py \
    --student template \
    --mnist ../Data --num-samples -1 --random-seed 0 \
    --batch-size 300 --num-workers 4  \
    --kernel 5 --stride 1    \
    --optim-alg sgd --learning-rate 0.0001 --l2-lambda 0. \
    --num-epochs 100 --device cuda
